KC_HEALTH_ENABLED=true
KC_METRICS_ENABLED=true
KC_FEATURES=token-exchange
KC_HOSTNAME_STRICT=false
KC_LOG_LEVEL=INFO

KC_BOOTSTRAP_ADMIN_USERNAME=admin
KC_BOOTSTRAP_ADMIN_PASSWORD=admin



# Database Configuration
# Copy this file to .env and update with your actual database credentials

# PostgreSQL Database URL
DB_URL=jdbc:postgresql://192.168.1.23:5432/obp_mapped

# Database Username
DB_USER=obp

# Database Password
DB_PASSWORD=f

# Optional: Database Driver (usually doesn't need to be changed)
DB_DRIVER=org.postgresql.Driver

# Optional: Hibernate Dialect (usually doesn't need to be changed)
DB_DIALECT=org.hibernate.dialect.PostgreSQLDialect

# Optional: Hibernate DDL Auto (validate, update, create, create-drop)
HIBERNATE_DDL_AUTO=validate

# Optional: Show SQL queries in logs (true/false)
HIBERNATE_SHOW_SQL=true

# Optional: Format SQL queries in logs (true/false)
HIBERNATE_FORMAT_SQL=true
